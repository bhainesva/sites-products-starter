{
  "templateVersion": 8,
  "features": [
    {
      "name": "Products",
      "streamId": "products",
      "templateType": "SOY",
      "entityPageSet": {
        "htmlTemplate": "locationEntity.yexter",
        "urlTemplate": "url.yexter"
      }
    },
    {
      "name": "Landing Page",
      "templateType": "SOY",
      "staticPage": {
